<%= form_for(@pairing) do |f| %>
  <div class = "field">
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :item_array, "Items" %>
    <%= f.hidden_field :item_array %>
    <%= f.label :tag_list, "Tags" %>
    <%= f.hidden_field :tag_list %>
  </div>
  <%= f.submit "Add Pairing", class: "btn btn-large btn-primary" %>
<% end %>

<%= s3_uploader_form do %>
  <%= file_field_tag :file, multiple: true %>
  <% end %>

  <script id="template-upload" type="text/x-tmpl">
<div class = "upload">
{%=o.name%}
<div class="progress"><div class="bar" style="width: 0%"></div></div></div>
  </script>
